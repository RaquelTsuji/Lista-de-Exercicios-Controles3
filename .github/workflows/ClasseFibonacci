public class Fibonacci {
	private int fib;
	private int fib1 = 0;
	private int fib2 = 1;
	
	public Fibonacci (int numero){
		for (int counter=0; counter<numero; counter++) {
			fib=fib1+fib2;
			fib1=fib2;
			fib2=fib;
			System.out.printf ("%dº Número de Fibonacci: %d\n", counter+1, fib);
		}
	}
}
