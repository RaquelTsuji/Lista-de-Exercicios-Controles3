import java.util.Scanner;
public class MainFibonacci {
	public static int numero;
	public static void main(String[] args) {
		while (numero!=-1) {
			System.out.print ("\nNúmero de Fibonacci");
			
			Scanner sc= new Scanner(System.in);
			System.out.print ("\nEntre com o número de termos ou saia com -1: ");
			numero=sc.nextInt();
			
			System.out.print ("\n");
			
			new Fibonacci (numero);
		}
	}
}
